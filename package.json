{
  "name": "express-prisma-mongo-starter",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "prisma db push && nodemon ./src/index",
    "start": "prisma db push && node ./src/index",
    "mongo-start": "docker run -d -p 27017-27019:27017-27019 --name mongo-dev adaptable/mongo-replica-set",
    "mongo-stop": "docker rm -f mongo-dev"
  },
  "dependencies": {
    "@prisma/client": "^3.14.0",
    "axios": "^1.6.8",
    "discord-webhook-node": "^1.1.8",
    "discord.js": "^14.14.1",
    "express": "~4.19.1",
    "log4js": "^6.7.1",
    "minecraft-data": "^3.27.0",
    "mineflayer": "^4.8.1",
    "mineflayer-pathfinder": "^2.4.2",
    "pastebin-js": "^1.0.6",
    "socket.io": "^4.7.5",
    "ngrok": "latest",
    "localtunnel": "latest",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^16.0.1",
    "morgan": "~1.9.1",
    "prisma": "^3.14.0",
    "sanitize-html": "^2.7.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.16"
  },
  "nodemonConfig": {
    "ext": "js,mjs,json,css,html"
  }
